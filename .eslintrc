{
    "extends": "airbnb",
    "env": {
      "browser": true,
      "node": true,
      "mocha": true
    },
    "rules": {
      "no-console": "off",
      "comma-dangle": "off",
      "react/jsx-filename-extension": "off",
      "import/no-extraneous-dependencies": ["error", {"devDependencies": true}],
      "linebreak-style": ["error", "windows"]
    }
  }